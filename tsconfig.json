{
  "extends": "@tsconfig/react-native/tsconfig.json" /* Recommended React Native TSConfig base */,
  // "include": ["./src/**/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
      "strict": true,
      "jsx": "react-native",
      "target": "esnext",
      "lib": ["dom", "es2015"],
      "allowJs": true,
      "skipLibCheck": true,
      "noEmit": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "types": ["jest", "node"],
      "paths": {
          "@/*": ["./src/*"]
      }
  }
}
